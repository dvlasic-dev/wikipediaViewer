{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
            0,{
              "vars": "all",
              "args": "after-used"
            }],
        "no-console":[
          "error", {
            "allow": ["warn", "error"] }] 

    }
}
